<table>
  <thead>
    <tr>
      <th>Clientid</th>
      <th>Teamid</th>
      <th>Netamount</th>
      <th>Grossamount</th>
      <th>Description</th>
      <th>Starttime</th>
      <th>Stoptime</th>
      <th>Materials</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_orders.each do |job_order| %>
      <tr>
        <td><%= job_order.clientId %></td>
        <td><%= job_order.teamId %></td>
        <td><%= job_order.netAmount %></td>
        <td><%= job_order.grossAmount %></td>
        <td><%= job_order.description %></td>
        <td><%= job_order.startTime %></td>
        <td><%= job_order.stopTime %></td>
        <td><%= job_order.materials %></td>
        <td><%= link_to 'Show', job_order %></td>
        <td><%= link_to 'Edit', edit_job_order_path(job_order) %></td>
        <td><%= link_to 'Destroy', job_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
