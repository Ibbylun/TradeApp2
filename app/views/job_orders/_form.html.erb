<%= simple_form_for(@job_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :clientId %>
    <%= f.input :teamId %>
    <%= f.input :netAmount %>
    <%= f.input :grossAmount %>
    <%= f.input :description %>
    <%= f.input :startTime %>
    <%= f.input :stopTime %>
    <%= f.input :materials %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
